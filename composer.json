{
  "name" : "lastov-dmitrii/ltree-extension-bundle",
  "description" : "Symfony 4 bundle with additional functionality for PostgreSQL ltree extension",
  "version" : "0.2.1",
  "type" : "symfony-bundle",
  "homepage": "https://github.com/lastov-dmitrii/ltree-extension-bundle",
  "license" : "MIT",
  "keywords" : ["ltree", "postgresql", "ltree-extension-bundle"],
  "authors" : [{
    "name" : "Dmtrii Lastov",
    "email" : "d.d.lastov@gmail.com"
  }],
  "require" : {
    "php": ">=7.2",
    "doctrine/orm": "~2.2",
    "doctrine/doctrine-bundle": "~1.2",
    "symfony/property-access": "~4",
    "symfony/config": "~4",
    "symfony/dependency-injection": "~4",
    "symfony/http-kernel": "~4"
  },
  "autoload" : {
    "psr-4" : { "DDL\\LtreeExtensionBundle\\" : "" }
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "0.1.x-dev"
    }
  },
  "require-dev": {
    "doctrine/coding-standard": "^7.0",
    "squizlabs/php_codesniffer": "^3.5"
  }
}
