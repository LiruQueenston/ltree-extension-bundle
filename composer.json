{
  "name" : "lastov-dmitrii/ltree-extension-bundle",
  "description" : "Symfony 4 bundle with additional functionality for PostgreSQL ltree extension",
  "version" : "0.3.0",
  "type" : "symfony-bundle",
  "homepage": "https://github.com/lastov-dmitrii/ltree-extension-bundle",
  "license" : "MIT",
  "keywords" : ["ltree", "postgresql", "ltree-extension-bundle"],
  "authors" : [{
    "name" : "Dmtrii Lastov",
    "email" : "d.d.lastov@gmail.com"
  }],
  "require" : {
    "php": ">=7.3",
    "doctrine/orm": "^2.7",
    "doctrine/doctrine-bundle": "^2.0",
    "symfony/property-access": "^4.4|^5.0",
    "symfony/config": "^4.4|^5.0",
    "symfony/dependency-injection": "^4.4|^5.0",
    "symfony/http-kernel": "^4.4|^5.0"
  },
  "autoload" : {
    "psr-4" : { "DDL\\LtreeExtensionBundle\\" : "lib" }
  },
  "autoload-dev": {
    "psr-4": { "DDL\\Tests\\": "tests" }
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "master"
    }
  },
  "require-dev": {
    "doctrine/coding-standard": "^7.0",
    "squizlabs/php_codesniffer": "^3.5",
    "jetbrains/phpstorm-stubs": "^2019.1",
    "phpstan/phpstan": "^0.11.6",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpunit/phpunit": "^8.4.1"
  }
}
